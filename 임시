  from crewai import Agent, Task, Crew, Process

# 1. ì—­í•  ì •ì˜
planner = Agent(
    role="Planner",
    goal="ë¡œë˜ ë°ì´í„° ìˆ˜ì§‘, ì €ì¥, ë¶„ì„, ì¶”ì²œ, ì¶”ì  ê´€ì°°ì˜ ì „ì²´ì ì¸ ì›Œí¬í”Œë¡œìš° ì„¤ê³„",
    backstory="ë‹¹ì‹ ì€ ë°ì´í„° ì‚¬ì´ì–¸ìŠ¤ í”„ë¡œì íŠ¸ë¥¼ ê¸°íší•˜ëŠ” ì „ë¬¸ í”„ë¡œì íŠ¸ ë§¤ë‹ˆì €ë‹¤."
)

researcher = Agent(
    role="Researcher",
    goal="ë¡œë˜ ë‹¹ì²¨ ë²ˆí˜¸ ë°ì´í„°ë¥¼ ìˆ˜ì§‘í•˜ê³  DBì— ì €ì¥í•˜ëŠ” ë°©ë²•ì„ ì¡°ì‚¬í•˜ê³  ì •ë¦¬í•œë‹¤",
    backstory="ë‹¹ì‹ ì€ ì›¹ í¬ë¡¤ë§ê³¼ ë°ì´í„° ìˆ˜ì§‘ ê²½í—˜ì´ í’ë¶€í•œ ë°ì´í„° ì—”ì§€ë‹ˆì–´ë‹¤."
)

analyst = Agent(
    role="Analyst",
    goal="ì €ì¥ëœ ë¡œë˜ ë‹¹ì²¨ ë²ˆí˜¸ ë°ì´í„°ë¥¼ í†µê³„ì  ê¸°ë²•, íŒ¨í„´ ë¶„ì„, ë¨¸ì‹ ëŸ¬ë‹ ë“±ì„ ì´ìš©í•´ ë¶„ì„í•˜ê³  ì¶”ì²œ ë²ˆí˜¸ë¥¼ ìƒì„±í•œë‹¤",
    backstory="ë‹¹ì‹ ì€ ìˆ«ì íŒ¨í„´ ë¶„ì„ê³¼ ì˜ˆì¸¡ ëª¨ë¸ë§ì„ ì˜í•˜ëŠ” ë°ì´í„° ê³¼í•™ìë‹¤."
)

tracker = Agent(
    role="Tracker",
    goal="ì¶”ì²œí•œ ë²ˆí˜¸ê°€ ì‹¤ì œ ë‹¹ì²¨ ë²ˆí˜¸ì™€ ì–¼ë§ˆë‚˜ ì¼ì¹˜í•˜ëŠ”ì§€ ì¶”ì í•˜ê³ , ì„±ëŠ¥ ë¦¬í¬íŠ¸ë¥¼ ì œê³µí•œë‹¤",
    backstory="ë‹¹ì‹ ì€ ë°ì´í„° ëª¨ë‹ˆí„°ë§ê³¼ ì„±ëŠ¥ í‰ê°€ë¥¼ ì „ë¬¸ìœ¼ë¡œ í•˜ëŠ” ë¶„ì„ê°€ë‹¤."
)

# 2. Task ì •ì˜
plan_task = Task(
    description=(
        "1) ë¡œë˜ ë‹¹ì²¨ ë²ˆí˜¸ ìˆ˜ì§‘ ë° DB ì €ì¥ ë°©ì‹, "
        "2) ë§¤ì£¼ ì—…ë°ì´íŠ¸ ì „ëµ, "
        "3) ë¶„ì„/ì¶”ì²œ ë°©ë²•ë¡ , "
        "4) ì¶”ì²œ ë²ˆí˜¸ ì¶”ì  ë° ì„±ëŠ¥ ê´€ë¦¬ ë°©ë²•ì„ í¬í•¨í•œ ì›Œí¬í”Œë¡œìš°ë¥¼ ì„¤ê³„í•˜ë¼."
    ),
    agent=planner
)

research_task = Task(
    description="ë¡œë˜ ë‹¹ì²¨ ë²ˆí˜¸ë¥¼ ìˆ˜ì§‘í•˜ê³  DB(MySQL ë˜ëŠ” SQLite)ì— ì €ì¥í•  ìˆ˜ ìˆëŠ” ë°©ë²•ê³¼ ì½”ë“œ ì˜ˆì‹œë¥¼ ì œê³µí•˜ë¼.",
    agent=researcher
)

analysis_task = Task(
    description="DBì— ì €ì¥ëœ ë°ì´í„°ë¥¼ ë¶ˆëŸ¬ì™€ ë‹¹ì²¨ë²ˆí˜¸ë¥¼ í†µê³„ì ìœ¼ë¡œ ë¶„ì„í•˜ê³ , ì°¨ê¸° íšŒì°¨ ì¶”ì²œ ë²ˆí˜¸ 6ê°œë¥¼ ìƒì„±í•˜ë¼.",
    agent=analyst
)

tracking_task = Task(
    description="ì¶”ì²œëœ ë²ˆí˜¸ì™€ ì‹¤ì œ ë‹¹ì²¨ë²ˆí˜¸ë¥¼ ë¹„êµí•˜ëŠ” ì¶”ì  ë°©ë²•ì„ ì„¤ê³„í•˜ê³ , ì •í™•ë„ë¥¼ ê¸°ë¡í•˜ëŠ” DB í…Œì´ë¸” êµ¬ì¡°ë¥¼ ì œì•ˆí•˜ë¼.",
    agent=tracker
)

# 3. Crew ì •ì˜
crew = Crew(
    agents=[planner, researcher, analyst, tracker],
    tasks=[plan_task, research_task, analysis_task, tracking_task],
    process=Process.sequential
)

# 4. ì‹¤í–‰
result = crew.kickoff(inputs={"topic": "ë¡œë˜ ë°ì´í„° ê¸°ë°˜ ë²ˆí˜¸ ì¶”ì²œ ì‹œìŠ¤í…œ"})
