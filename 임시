# lotto_crew_scheduler.py
import schedule
import time
import requests
from crewai import Agent, Task, Crew, Process

# -----------------------------
# 1ï¸âƒ£ CrewAI ì—­í•  ì •ì˜
# -----------------------------
planner = Agent(
    role="Planner",
    goal="ë¡œë˜ ë°ì´í„° ìˆ˜ì§‘, ë¶„ì„, ì¶”ì²œ, ì¶”ì  ê´€ì°° ì›Œí¬í”Œë¡œìš° ì„¤ê³„",
    backstory="ë°ì´í„° í”„ë¡œì íŠ¸ ê¸°íš ì „ë¬¸ê°€"
)

researcher = Agent(
    role="Researcher",
    goal="ë¡œë˜ ë‹¹ì²¨ ë²ˆí˜¸ë¥¼ ìˆ˜ì§‘í•˜ê³  DBì— ì €ì¥í•˜ëŠ” ë°©ë²• ì¡°ì‚¬",
    backstory="ë°ì´í„° ì—”ì§€ë‹ˆì–´"
)

analyst = Agent(
    role="Analyst",
    goal="ì €ì¥ëœ ë¡œë˜ ë²ˆí˜¸ë¥¼ í†µê³„/íŒ¨í„´ ë¶„ì„ í›„ ì¶”ì²œ ë²ˆí˜¸ ìƒì„±",
    backstory="ë°ì´í„° ê³¼í•™ì"
)

tracker = Agent(
    role="Tracker",
    goal="ì¶”ì²œ ë²ˆí˜¸ì™€ ì‹¤ì œ ë‹¹ì²¨ë²ˆí˜¸ ë¹„êµ ë° ì •í™•ë„ ì¶”ì ",
    backstory="ë°ì´í„° ëª¨ë‹ˆí„°ë§ ì „ë¬¸ê°€"
)

# -----------------------------
# 2ï¸âƒ£ CrewAI Task ì •ì˜
# -----------------------------
plan_task = Task(
    description="ë¡œë˜ ë°ì´í„° ìˆ˜ì§‘, DB ì €ì¥, ì¶”ì²œ, ì¶”ì  ê´€ì°° ì›Œí¬í”Œë¡œìš° ì„¤ê³„",
    agent=planner
)

research_task = Task(
    description="DBì— ë¡œë˜ ë‹¹ì²¨ë²ˆí˜¸ ìˆ˜ì§‘/ì €ì¥ ë°©ë²•ê³¼ ì½”ë“œ ì˜ˆì‹œ ì œê³µ",
    agent=researcher
)

analysis_task = Task(
    description="DB ë°ì´í„°ë¥¼ ë¶„ì„í•˜ê³  ì¶”ì²œ ë²ˆí˜¸ 6ê°œ ìƒì„±",
    agent=analyst
)

tracking_task = Task(
    description="ì¶”ì²œ ë²ˆí˜¸ì™€ ì‹¤ì œ ë‹¹ì²¨ë²ˆí˜¸ ë¹„êµ ë° ê´€ë¦¬ ë°©ë²• ì œì•ˆ",
    agent=tracker
)

# -----------------------------
# 3ï¸âƒ£ Crew ì •ì˜
# -----------------------------
crew = Crew(
    agents=[planner, researcher, analyst, tracker],
    tasks=[plan_task, research_task, analysis_task, tracking_task],
    process=Process.sequential
)

# -----------------------------
# 4ï¸âƒ£ ì¹´í†¡ ë©”ì‹œì§€ ì „ì†¡ í•¨ìˆ˜
# -----------------------------
def send_kakao_message(message):
    url = "https://kakaoapi.example.com/send"  # ì•Œë¦¼í†¡ API ì˜ˆì‹œ
    headers = {"Authorization": "Bearer YOUR_ACCESS_TOKEN"}
    data = {
        "receiver": "01012345678",
        "message": message
    }
    requests.post(url, headers=headers, json=data)

# -----------------------------
# 5ï¸âƒ£ CrewAI ì‹¤í–‰ + ì¹´í†¡ ì „ì†¡
# -----------------------------
def run_lotto_workflow():
    print("CrewAI ì›Œí¬í”Œë¡œìš° ì‹¤í–‰ ì¤‘...")
    result = crew.kickoff(inputs={"topic": "ë¡œë˜ ë°ì´í„° ê¸°ë°˜ ë²ˆí˜¸ ì¶”ì²œ ì‹œìŠ¤í…œ"})
    print(result)
    
    # ê²°ê³¼ë¥¼ ì¹´í†¡ìœ¼ë¡œ ì „ì†¡
    send_kakao_message(f"ì´ë²ˆ ì£¼ ë¡œë˜ ì¶”ì²œ ë²ˆí˜¸:\n{result}")

# -----------------------------
# 6ï¸âƒ£ ìŠ¤ì¼€ì¤„ëŸ¬ ì„¤ì • (ë§¤ì£¼ ì›”ìš”ì¼ 09:00)
# -----------------------------
schedule.every().monday.at("09:00").do(run_lotto_workflow)

print("ë¡œë˜ ì¶”ì²œ ìë™í™” ìŠ¤ì¼€ì¤„ëŸ¬ ì‹œì‘...")
while True:
    schedule.run_pending()
    time.sleep(60)

